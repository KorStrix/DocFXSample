{
	"metadata": [
		{
			"src": [
				{
					"src": "..",
					"files": [
						"*.csproj"
					]
				}
			],
			"dest": "api",
			"filter": "filterConfig.yml"
		}
	],
	"build": {
		"globalMetadata": {
			"_appTitle": "Example Unity documentation",
			"_appFooter": "Example Unity documentation",
			"_enableSearch": true,
			    "_gitContribute": {
				"repo": "https://github.com/KorStrix/DocFXSample.git",
				"branch": "master",
				"path": ""
				}
		},
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
                "src": "api",
                "files": [ "*.yml" ],
                "dest": "api"
            },
            {
                "src": "manual",
                "files": [ "toc.yml", "*.md" ],
                "dest": "manual"
            }
		],
		"overwrite": [
            {
                "src": "..",
                "files": [ "Assets/Scripts/**/*.md" ]
            }
		],
		"resource": [
			{
				"files": [
					"images/**"
				]
			}
		],
		"dest": "_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"default",
			"templates/unity"
		],
		"postProcessors": [],
		"markdownEngineName": "markdig",
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false,
		"disableGitFeatures": false,
		"sitemap": {
			"baseUrl": "https://korstrix.github.io/docfxsample"
		},
        "dest": "../_site"
	}
}
